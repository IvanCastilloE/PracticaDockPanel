<Window x:Class="PracticaDockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaDockPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Left">Botonazo 1</Button>
        <Button DockPanel.Dock="Left" >Botonazo 5</Button>
        <Button DockPanel.Dock="Right">Botonazo 3</Button>
        <Button DockPanel.Dock="Top">Botonazo 2</Button>
        <Button DockPanel.Dock="Bottom">Botonazo 4</Button>
        
        <StackPanel>
            <TextBlock Text="Un textito"/>
            <TextBox/>
            <TextBlock Text="Otro textito"/>
            <Button>Botonazo 6</Button>
        </StackPanel>
    </DockPanel>
</Window>
